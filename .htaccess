RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([^\.]+)$ $1.php [NC,L]
RewriteRule ^([^\.]+)$ $1.html [NC,L]

RewriteCond %{HTTP_HOST} ^mx.plugger\.tk [NC]
RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ http://mx.plugger.tk/$1 [R,L]

